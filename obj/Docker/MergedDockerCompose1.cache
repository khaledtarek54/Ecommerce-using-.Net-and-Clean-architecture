        source: C:\Users\Khaled\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Khaled\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: E:\.net projects\Ecommerce-project\Ecommerce
        target: /app
        bind:
          create_host_path: true
      - type: bind
        source: E:\.net projects\Ecommerce-project
        target: /src
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Khaled\vsdbg\vs2017u5
        target: /remote_debugger
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Khaled\.nuget\packages\
        target: /.nuget/packages
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Khaled\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Khaled\AppData\Roaming/Microsoft/UserSecrets
